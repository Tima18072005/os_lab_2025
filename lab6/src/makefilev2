CC = gcc
CFLAGS = -Wall -Wextra -O2

all: clientv2 serverv2

clientv2: clientv2.c utils.o
	$(CC) $(CFLAGS) -o clientv2 clientv2.c utils.o

serverv2: serverv2.c utils.o
	$(CC) $(CFLAGS) -o serverv2 serverv2.c utils.o
        
utils.o: utils.c utils.h
	$(CC) $(CFLAGS) -c utils.c 

clean:
	rm -f clientv2 serverv2 utils.o
